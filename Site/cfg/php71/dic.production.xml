<?xml version="1.0" encoding="UTF-8" ?>
<!-- 
    DI container definition - production use 
-->
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <imports>
        <import resource="dic.slim.xml"/>
        <import resource="parameters.production.yml"/>
        <import resource="dic.base.factory.xml"/>
        <import resource="dic.base.logging.xml"/>
        <import resource="dic.base.controllers.xml"/>
        <import resource="dic.base.models.xml"/>
        <import resource="dic.base.view.xml"/>
    </imports>

</container>